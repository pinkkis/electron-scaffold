{
  "name": "electron_scaffold",
  "version": "0.1.0",
  "description": "electron app scaffold with angular 1.5 and material",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "bower install && typings install",
    "dist:win64": "build --platform win32 --arch x64",
    "run": "electron ./app",
    "pack": "build --target dir",
    "dist": "build"
  },
  "keywords": [
    "electron",
    "template",
    "scaffold",
    "angular",
    "material",
    "desktop"
  ],
  "author": "Kristian Koivisto-Kokko",
  "license": "MIT",
  "devDependencies": {
    "asar": "^0.11.0",
    "electron-builder": "^5.2.0",
    "electron-prebuilt": "^1.2.2",
    "event-stream": "^3.3.2",
    "gulp": "^3.9.1",
    "gulp-inject": "^4.1.0",
    "gulp-less": "^3.1.0",
    "main-bower-files": "^2.13.1",
    "rcedit": "^0.5.0"
  },
  "dependencies": {},
  "build": {
    "compression": "store",
    "productName": "Electron Scaffold",
    "win": {
      "remoteReleases": "https://github.com/pinkkis/electron-scaffold",
      "iconUrl": "https://github.com/pinkkis/electron-scaffold/blob/master/build/icon.ico?raw=true"
    }
  }
}
